double TLab34NProj::mercatorLat( double lat ) const {
\end_layout

\begin_layout Plain Layout

   lat = lat * atan2(1,1) / 90;
\end_layout

\begin_layout Plain Layout

   double mLat = log( tan( 0.5*lat + atan2(1,1) ) );
\end_layout

\begin_layout Plain Layout

   mLat = 90 * mLat / atan2(1,1);
\end_layout

\begin_layout Plain Layout

   return mLat;
\end_layout

\begin_layout Plain Layout

};
