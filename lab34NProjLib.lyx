\end_layout

\begin_layout Plain Layout

double TLab34NProj::unmercatorLat( double mlat ) const {
\end_layout

\begin_layout Plain Layout

   mlat = mlat * atan2(1,1) / 90;
\end_layout

\begin_layout Plain Layout

   double lat = 2.0 *( atan(exp(mlat)) - atan2(1,1) ) ;
\end_layout

\begin_layout Plain Layout

   lat = 90 * lat / atan2(1,1);
\end_layout

